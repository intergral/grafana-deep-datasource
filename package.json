{
  "name": "grafana-deep-datasource",
  "version": "1.0.2",
  "description": "Datasource plugin for DEEP",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix",
    "prettier": "prettier -w .",
    "fmt-check": "prettier -c .",
    "e2e": "yarn exec cypress install && yarn exec grafana-e2e run",
    "e2e:update": "yarn exec cypress install && yarn exec grafana-e2e run --update-screenshots",
    "server": "docker-compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "zip": "./build_zip.sh",
    "docs": "mkdocs serve -f docs/mkdocs.yml"
  },
  "author": "Intergral GmbH",
  "license": "AGPL-3.0",
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@grafana/e2e": "10.3.3",
    "@grafana/e2e-selectors": "10.3.3",
    "@grafana/eslint-config": "^7.0.0",
    "@grafana/tsconfig": "^1.3.0-rc1",
    "@swc/core": "^1.3.90",
    "@swc/helpers": "^0.5.6",
    "@swc/jest": "^0.2.36",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.19",
    "@types/prismjs": "^1.26.3",
    "@types/react-router-dom": "^5.2.0",
    "@types/slate": "0.47.11",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.10.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-jsdoc": "48.1.0",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "glob": "^10.3.10",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.5",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.71.1",
    "sass-loader": "14.1.1",
    "style-loader": "3.3.4",
    "swc-loader": "^0.2.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@emotion/css": "^11.11.2",
    "@grafana/data": "10.4.0",
    "@grafana/experimental": "^1.7.10",
    "@grafana/runtime": "10.3.3",
    "@grafana/schema": "10.3.3",
    "@grafana/ui": "10.3.3",
    "prismjs": "^1.29.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "slate": "0.47.9",
    "tslib": "2.6.2",
    "uuid": "^9.0.1"
  }
}
